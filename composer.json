{
    "name": "zanderwar/lumen-dropbox",
    "description": "A Lumen Dropbox v2 package",
    "license": "MIT",
    "authors": [
        {
            "name": "David Carr",
            "email": "dave@dcblog.dev",
            "homepage": "https://dcblog.dev"
        },
        {
            "name": "Reece Alexander",
            "email": "reece.alexander@gmail.com"
        }
    ],
    "homepage": "https://github.com/zanderwar/lumen-dropbox",
    "keywords": ["Lumen", "Dropbox"],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0",
        "guzzlehttp/guzzle": "^6|^7"
    },
    "autoload": {
        "psr-4": {
            "Zanderwar\\Dropbox\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zanderwar\\Dropbox\\DropboxServiceProvider"
            ],
            "aliases": {
                "Dropbox": "Zanderwar\\Dropbox\\Facades\\Dropbox"
            }
        }
    }
}
